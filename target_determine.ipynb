    "function determine_target(cost,current_bid,method=\"penetrative\")\n",
    "    if method==\"penetrative\" \n",
    "        for i in 35:-1:1\n",
    "            cost[i]=min(cost[i],cost[i+1]);\n",
    "        end\n",
    "        return cost;\n",
    "    end\n",
    "    \n",
    "    #to do implement classic bellman equation\n",
    "    return cost;\n",
    "    \n",
    "end\n"
   ]
